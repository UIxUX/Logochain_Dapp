<!DOCTYPE html>
<html lang="en">
<%include header%>
<body>

<!-- Navigationbar -->
<nav class="navbar navbar-expand-lg transparentBackground">
  <a class="navbar-brand" href="/">
    <img src="/images/logo.svg" width="150" height="26" class="d-inline-block align-top" alt="">
  </a>
  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>
  <div class="collapse navbar-collapse" id="navbarSupportedContent">

    <ul class="navbar-nav mr-auto ul-navbar" >
      <li class="nav-item li-navbar">
        <a class="nav-link signOutNavbar" href="/"><b>Front Page</b><span class="sr-only">(current)</span></a>
      </li>

      <!--
      <li class="nav-item active li-navbar"">
      <a class="nav-link uploadButton" href="#">Upload <span class="sr-only">(current)</span></a>
      </li>
      -->

      <li class="nav-item li-navbar">
        <a class="nav-link signOutNavbar" href="/logout">Sign Out <span class="sr-only">(current)</span></a>
      </li>
    </ul>

  </div>
</nav>
<!-- Account Information Section -->
<section>
  <main id="accountContainer" class="lightShadow">
    <div id="accountContainerHeader">
      <h5 id="yourAccount" class="whiteText">Your Account: <%= username%></h5>
      <a id="deleteAccountButton" type="POST" href="#" class="whiteOutlinedButton btn round deleteAccount">Delete</a>
      <h4 id="walletID" class="whiteText"><%= wallet_id%></h4>
    </div>
    <div id="accountContainerMain">
      <ul id="accountContainerMainList">
        <li id="accountEmail" class="accountContainerListItem verticallycenterchildren">
          <h4>Email:</h4>
          <form class="form-inline my-2 my-lg-0 ">
            <input class="form-control mr-sm-2 searchBox inputBox round" type="email" placeholder="<%= email%>" aria-label="Email">
          </form>
          <button class="editButton btn" id="editButtonEmail">Edit</button>
        </li>
        <li id="accountPassword" class="accountContainerListItem verticallycenterchildren">
          <h4>Password:</h4>
          <form class="form-inline my-2 my-lg-0 ">
            <input class="form-control mr-sm-2 searchBox inputBox round" type="password" placeholder="<%= password%>" aria-label="Email">
          </form>
          <button class="editButton btn" id="editButtonPassword">Edit</button>
        </li>
      </ul>
    </div>
  </main>
</section>

<section class="accountDesignsSection">
  <div class="topContainer">
    <div class="topContainerInnerBox">
      <h3>ðŸ‘Œ Logos you have upvoted:</h3>
    </div>
  </div>

  <!-- You upvoted Section -->
  <div class="mainSubmissionsContainer" class="group">
    <div class="allSubmissionsContainer">
      <ul class="submissionsList">

          <% for(var i=0; i< upvotedSubs.length; i++) {%>
        <li class="submission round">
          <img class="submissionImg inSubmissionElement" width="90" height="90" class="img-circle" src="data:image/jpeg;base64, <%=upvotedSubs[i].icon.data %>" alt="Your Logo Alt" >
          <h4><%=upvotedSubs[i].title %></h4>
          <h6 class=""><img class="inSubmissionElement" width="8" height="14" class="" src="/images/ethIconSmall.png" alt="Your Logo Alt" > <%=upvotedSubs[i].price %> Ether</h6>
          <button class="btn upvoteButton round inSubmissionElement">â–´ <%=upvotedSubs[i].upvotes.length %> Upvote</button>
          <button class="btn buyButton round inSubmissionElement">Buy</button>
        </li>
          <%} %>
        <!-- Test -->

      </ul>
    </div>
  </div>
</section>


